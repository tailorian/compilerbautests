f(x : array of array of int)
    if x[0][1] < 1 then
        var y : array of int := x[0]; 
        var x : int := 1;
        while x # y[x] do
            x := (y[x] + 1) * 3;
        end;
        return 1;
    else
        if x[0][0] < 0 then
            return 2;
        end;
    end;

    while f(x) : array of int[0] < x[0][1] do
        var x : int := 1;
    end;
end;
